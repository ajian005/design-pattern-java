public class GuanYin {
    private List<Observer> observers =3D new ArrayList<Observer>();

    public void watering(){
        System.out.println(&quot;=E8=A7=82=E9=9F=B3=E6=B4=92=E6=B0=B4&quot;=
);
        notifyAllTortoise();
    }

    private void notifyAllTortoise() {
        int i =3D 0;
        for (Observer o:observers) {
            o.flyToGuanYin();
            System.out.println(++i);
        }
    }

    public void attach(Observer observer) {
        observers.add(observer);
    }
}


public abstract class Observer {
    protected GuanYin guanYin;
    protected abstract void flyToGuanYin();
